{
  "name": "@itsib/icon-font",
  "description": "CLI utilities for icon-font generation, automation and presentation. Includes preview web server.",
  "version": "0.2.14",
  "license": "MIT",
  "type": "module",
  "private": false,
  "keywords": [
    "font",
    "ttf",
    "eot",
    "woff",
    "woff2",
    "iconfont",
    "icon-font"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsib/icon-font.git"
  },
  "bin": {
    "icon-font": "icon-font",
    "dump": "dump"
  },
  "main": "./icon-font",
  "files": [
    "lib/",
    "icon-font",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "dev": "npm run inject && vite",
    "inject": "node scripts/inject.mjs",
    "build": "npm run inject && tsc && vite build",
    "preversion": "echo \"releases\"",
    "version": "node ./scripts/version.mjs && git add .",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/node": "^20.17.9",
    "@types/sax": "^1.2.7",
    "@types/ttf2eot": "^2.0.2",
    "@types/ttf2woff": "^2.0.4",
    "@types/wawoff2": "^1.0.2",
    "commander": "^12.1.0",
    "prettier": "^3.3.1",
    "sax": "^1.4.1",
    "svg-pathdata": "^7.1.0",
    "ttf2eot": "^3.1.0",
    "ttf2woff": "^3.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "wawoff2": "^2.0.1"
  }
}
